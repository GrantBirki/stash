    "dotenv": "^16.4.5",
    "got": "^13.0.0",
    "i18next": "^23.11.3",
    "moment": "^2.30.1",
    "nodemon": "^3.1.0",
    "turndown": "^7.1.3",
    "uuid": "^9.0.1"
